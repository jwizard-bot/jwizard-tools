{
  "psi-header.config": {
    "forceToTop": true
  },
  "psi-header.changes-tracking": {
    "isActive": true,
    "autoHeader": "autoSave",
    "include": [
      "python"
    ],
    "excludeGlob": [
      ".github/**/*",
      ".venv/**/*",
      ".vscode/**/*"
    ],
    "enforceHeader": true
  },
  "psi-header.lang-config": [
    {
      "language": "python",
      "begin": "#",
      "prefix": "# ",
      "end": "#",
      "lineLength": 64
    }
  ],
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "Copyright (c) <<year>> by JWizard",
        "Originally developed by Mi≈Çosz Gilga <https://miloszgilga.pl>"
      ]
    }
  ]
}